{
  "version": 2,
  "builds": [
    { "src": "Frontend/website-lead-dashboard/package.json", "use": "@vercel/next" },
    { "src": "Backend/api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/api/backend/(.*)",
      "dest": "Frontend/website-lead-dashboard/.next/server/app/api/backend/$1/route"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/website-lead-dashboard/$1"
    }
  ],
  "functions": {
    "Frontend/website-lead-dashboard/src/app/api/backend/[...path]/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "Backend/api/index.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}
